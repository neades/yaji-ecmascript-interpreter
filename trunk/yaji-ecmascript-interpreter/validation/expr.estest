function isNaN(x) {
  var s = new String(x);
  return s=="NaN"
}

o = new Object();
undefined=o.undefined;

// 11

// 11.1
@test primary.1
a = 1;
this.a == 1;

@test primary.2
b = "string";
(((b))) == "string";

// 11.2
@test left-hand-side.1
c = new Object();
c["1"] = 2; 
2 == c[1];

@test left-hand-side.2
d = new String("xx");
d == "xx";

// 11.3
@test postfix.1
e = 2;
e++; e++; e++; e--;
e == 4;

// 11.4
@test unary.1
"number" == typeof 3;

@test unary.2
e = 2;
f = e++;
(f+1) == e;


@test unary.3
e = 2;
f = e--;
f == e+1;

@test unary.4
e = -4;
f = +e;
f == e;

@test unary.5
e = -4;
f = -e;
f+e == 0;

@test unary.6
e = 0xFF;
f = ~ e;
e==~~e && f!=e

